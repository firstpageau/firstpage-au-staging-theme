        <div class="hero-img d-none d-md-block d-lg-none">
            <img src="{{ site.theme.link }}/assets/img/img_amazon_hero-tablet@2x.png"
                srcset="{{ site.theme.link }}/assets/img/img_amazon_hero-tablet@2x.png, {{ site.theme.link }}/assets/img/img_amazon_hero-tablet@2x.png 2x"
