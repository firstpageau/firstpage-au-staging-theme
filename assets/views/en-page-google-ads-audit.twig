{% extends "en-base.twig" %}

{% block content %}

    <style>

        .bottom-get-audit-btn-holder {
            margin-bottom: 90px;
            padding-bottom: 70px;
        }

        .get-audit-btn {
            width: 66%;
        }
        .audit-template-hero-img-lg {
            margin-right: 90px;
        }

        .google-ads-audit-hero-img-lg-holder {
            bottom: -96px !important;
        }

        .audit-right-img {
            max-width: 548px;
        }
        .get-audit-btn{
            padding: 24px 26px;
        }

        .image-container-right img {
            max-width: 80%;
        }

        @media (max-width: 1600px){
            .audit-template-hero-img-lg {
                max-height: 485px;
                margin-right: 45px;
            }

        }

        @media (max-width: 1200px){
            .page-google-ads-audit .hero-img{
                right: -340px !important;
            }

        }

        @media (max-width: 992px){
            .page-google-ads-audit .quote-form{
                width: 610px;
            }
        }

        @media (max-width: 769px) {

            .page-google-ads section.banner .banner-description p {
                min-height: 267px;
            }

            .page-google-ads-audit .quote-form{
                width:100% !important;
                margin: auto;
            }

            .page-google-ads-audit .banner-hero-cont{
                max-width: 100% !important;
            }

            .page-google-ads-audit .awards-badges {
                text-align: center !important;

            }
        }

        @media (max-width: 576px){
            .audit-template-hero-mobile img {
                max-width: 350px;
                margin: auto;
                display: block;
            }

            .audit-template-hero-mobile img {
                max-width: 80%;
                margin-top: 35px;
            }

            .image-container-right img {
                width: 90%;
                margin-top: 35px;
            }
            .get-audit-btn {
                width: 95%;
            }
        }


    </style>

    <section class="banner form-space">
        <div class="container banner-hero">
            <div class="banner-hero-cont">
                <h1 class="form-hide">Get your FREE Google Ads Audit in less than 3 minutes</h1>
                <div class="banner-description-full-width">
                    <p class="form-hide">Stop guessing. Find out EXACTLY what is and isn’t working with your Google Ads with our newly refined PPC audit tool</p>
                    <div class="form-hide audit-template-hero-mobile d-block d-sm-none">
                        <img src="{{ site.theme.link }}/assets/img/img_google_ads_audit_au.svg"  alt="First Page" />
                    </div>
                </div>
                <div id="back-to-google-ads-audit-form"></div>
                {% include "partials/en-form-google-ads-audit.twig" with {'form_page': 'Google Ads Audit', 'form_button': 'Get My Free Audit', 'form_hs': 'hs_google_ads_audit_form'} %}
                {% include "partials/en-section-awards-badges.twig" %}
            </div>

            <div class="hero-img d-none d-lg-block google-ads-audit-hero-img-lg-holder">
