                $('#reviews-read-more-btn').addClass('d-block');
                $('#reviews-hide-on-mobile').removeClass('d-block');
                $('#reviews-hide-on-mobile').addClass('d-none');

                $([document.documentElement, document.body]).animate({
                    scrollTop: $("#reviews-read-more-btn").offset().top - 160
                }, 800);

            })
        });
    </script>

