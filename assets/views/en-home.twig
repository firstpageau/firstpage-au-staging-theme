<section class="banner form-space home-banner">
    <div class="container banner-hero">
        <div class="banner-hero-cont">
            <h1 class="form-hide">A smarter, faster way to drive traffic to your website.</h1>
            <div class="banner-description">
                <p class="form-hide">Get an integrated digital marketing strategy from APAC's highest rated digital agency.</p>
                <div class="form-hide hero-mobile">
                    <img src="{{ site.theme.link }}/assets/img/img-hero-fpa-home-mobile.png" srcset="{{ site.theme.link }}/assets/img/img-hero-fpa-home-mobile.png, {{ site.theme.link }}/assets/img/img-hero-fpa-home-mobile@2x.png 2x" alt="First Page" />
