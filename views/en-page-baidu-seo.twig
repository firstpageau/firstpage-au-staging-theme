{% extends "en-base.twig" %}

{% block content %}

<style>
    section.section-paragraph .p-0 {
        padding: 0 !important;
    }

    @media(max-width: 575px) {
        .text-center-mobile {
            text-align: center;
        }
    }

    @media(max-width: 575px) {
        .hero-mobile {
            position: unset !important;
            margin-bottom: -35px !important;
            text-align: center;
        }
    }

    .container-padding {
        padding: 0 34px;
    }

    @media(max-width: 767px) {
        .container-padding {
            padding: 0 15px;
        }
    }

    @media(max-width: 991px) {
        section.section-paragraph .text-container {
            padding: 0 15px 0 34px !important;
        }
    }

    @media(max-width: 767px) {
        section.section-paragraph .text-container {
            padding: 0 15px !important;
        }
    }

    section.section-paragraph .text-container-right {
        padding: 0 34px 0 15px;
    }

    section.section-paragraph .text-container-right .fp-btn {
        width: 100%;
        margin-top: 10px;
        margin-bottom: 30px;
    }

    @media(max-width: 767px) {
        .heading-center h2 {
            text-align: center;
        }
    }

    @media(max-width: 767px) {
        section.section-paragraph .text-container-right {
            padding: 0 15px;
        }
    }

    @media(max-width: 991px) {
        .input-group {
            display: -webkit-inline-box !important;
        }
    }

    @media(max-width: 767px) {
        .input-group {
            display: -webkit-flex !important;
        }

    }

    @media(min-width: 767px) {
        .quote-form.quote-form-big .fp-btn {
            padding: 0 24px !important;
        }
    }
</style>

<!-- Banner -->

<section class="banner form-space">
