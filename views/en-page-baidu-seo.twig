                        srcset="{{ site.theme.link }}/assets/img/img_hero_seo_mob@2x.png, {{ site.theme.link }}/assets/img/img_hero_seo_mob@2x.png 2x"
                        alt="First Page" />

                </div>
            </div>
            {% include "partials/en-form-services.twig" with {'form_page': 'Baidu SEO', 'form_button': 'Get A Free
            Quote', 'form_website_placeholder': 'Enter your website'} %}
            {% include "partials/en-section-awards-badges.twig" %}
