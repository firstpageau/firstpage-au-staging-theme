                $('#reviews-read-less-btn').removeClass('d-none');
                $('#reviews-read-less-btn').addClass('d-block d-md-none');

            })

            $('#reviews-read-less-btn').click(function(){
                $('#reviews-read-less-btn').removeClass('d-block');
                $('#reviews-read-less-btn').addClass('d-none');
