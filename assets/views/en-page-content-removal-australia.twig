{% extends "en-base.twig" %}

{% block content %}

<style>
    @media (max-width: 767px) {
        .page-content-removal-australia section.banner .banner-description p {
            min-height: 158px;
        }
    }
</style>

<section class="banner form-space">
    <div class="container banner-hero">
        <div class="banner-hero-cont">
            <h1 class="form-hide">Unwanted content removal to get your business back<br class="d-none d-lg-block"> on track</h1>
            <div class="banner-description">
                <p class="form-hide">Our cutting-edge technology will annihilate unfair content that is damaging<br class="d-none d-lg-block"> your brand</p>
                 <div class="form-hide hero-mobile">
                        <img src="{{ site.theme.link }}/assets/img/img_hero_sam_sm.png" srcset="{{ site.theme.link }}/assets/img/img_hero_sam_sm.png, {{ site.theme.link }}/assets/img/img_hero_sam_sm@2x.png 2x" alt="First Page" />
                </div>
            </div>
            {% embed "partials/en-form-services-content-removal-australia.twig" with {'form_page': 'Unwanted Content Removal', 'form_button': 'Get A Free Quote'} %}
            {% block step_1 %}
            <div class="input-group">
                <div class="select-container form-control">
                    <select class="form-control" name="removal_request">
                        <option value="Google Maps Review">Google Maps Review</option>
                        <option value="Google Images">Google Images</option>
                        <option value="Google Search Results">Google Search Results</option>
                        <option value="Glassdoor Review">Glassdoor Review</option>
                        <option value="Product Review">Product Review</option>
                        <option value="Ripoff Report">Ripoff Report</option>
                        <option value="Revenge Porn">Revenge Porn</option>
                        <option value="Something else...">Something else...</option>
                    </select>
                </div>
                <div class="input-group-btn"><button type="button" class="btn fp-btn fp-btn-orange form-continue">Get A Free Quote</button></div>
            </div>
            {% endblock %}
            {% endembed %}
            {% include "partials/en-section-awards-badges.twig" %}
        </div>
        <div class="hero-img d-none d-md-block d-lg-none">
                <img src="{{ site.theme.link }}/assets/img/img_hero_sam_md.png" srcset="{{ site.theme.link }}/assets/img/img_hero_sam_md.png, {{ site.theme.link }}/assets/img/img_hero_sam_md@2x.png 2x" alt="First Page" />
        </div>
        <div class="hero-img d-none d-lg-block">
                <img src="{{ site.theme.link }}/assets/img/img_hero_sam_lg.png" srcset="{{ site.theme.link }}/assets/img/img_hero_sam_lg.png, {{ site.theme.link }}/assets/img/img_hero_sam_lg@2x.png 2x" alt="First Page" />
        </div>
    </div>
</section>

