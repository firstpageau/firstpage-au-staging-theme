    <script>
        //back to query form anchor
        $(function () {
            $('.js-anchor-link').click(function(e){
                e.preventDefault();
                var target = $($(this).attr('href'));
                if(target.length){
                    $("html, body").animate({ scrollTop: $('#back-to-seo-audit-form').offset().top - 300 }, 1000);
                }
            });
        });

