<!DOCTYPE html>
{% block lang %}
<html lang="en">
  {% endblock %} {% block head %}

  <head>
    <title>
      {% if wp_title %}
      {{ wp_title }}
      {% else %}
      {{ site.name }}
      {% endif %}
    </title>
    <meta http-equiv="Content-Type" content="text/html; charset=UTF-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta
      name="viewport"
      content="width=device-width, initial-scale=1.0, maximum-scale=1, minimum-scale=1"
    />

    {% if meta_image %}
    <meta property="og:image" content="{{ meta_image }}" />
    {% else %}
    <meta
      property="og:image"
      content="{{ site.theme.link }}/assets/img/meta-img.png"
    />
    {% endif %}

    <meta
      name="facebook-domain-verification"
      content="iivsg55d4oxquyq9s1ep75gnk08nxf"
    />

    <!-- Core CSS -->
    <link
      rel="stylesheet"
      href="{{ site.theme.link }}/assets/css/bootstrap.min.css"
    />
    <link
      rel="stylesheet"
      href="{{ site.theme.link }}/assets/css/font-awesome.min.css"
    />
    <link rel="stylesheet" href="https://use.typekit.net/wqq0bzd.css" />
    <link
      rel="stylesheet"
      href="https://cdnjs.cloudflare.com/ajax/libs/magnific-popup.js/1.1.0/magnific-popup.min.css"
    />
    <link
      rel="stylesheet"
      href="https://unpkg.com/tippy.js@2.1.1/dist/tippy.css"
    />
    <link
      rel="stylesheet"
      href="https://cdnjs.cloudflare.com/ajax/libs/slick-carousel/1.9.0/slick.min.css"
    />
    <link
      rel="stylesheet"
      href="https://cdnjs.cloudflare.com/ajax/libs/slick-carousel/1.9.0/slick-theme.min.css"
    />

    <!-- Site CSS -->
    <link
      rel="stylesheet"
      href="{{ site.theme.link }}/style.css?v={{ asset_version }}"
    />

    <!-- Core JS -->
    <script src="{{ site.theme.link }}/assets/js/jquery-3.5.1.min.js"></script>
    <script src="https://code.jquery.com/jquery-migrate-1.4.1.min.js"></script>

    <script src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.12.9/umd/popper.min.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/parsley.js/2.8.0/parsley.min.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery-easing/1.4.1/jquery.easing.min.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/rangeslider.js/2.3.2/rangeslider.min.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/magnific-popup.js/1.1.0/jquery.magnific-popup.min.js"></script>
    <script src="https://unpkg.com/tippy.js@2.1.1/dist/tippy.standalone.min.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/ouibounce/0.0.12/ouibounce.min.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/moment.js/2.21.0/moment.min.js"></script>
    <script src="{{
        site.theme.link
      }}/assets/js/bootstrap.bundle.min.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/slick-carousel/1.9.0/slick.min.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/gsap/2.0.1/TweenMax.min.js"></script>

    <script>
      var wpSiteUrl = "{{ site.url }}";
      var wpHomeUrl = "{{ home_url }}";
      var wpAssetUrl = "{{ site.theme.link }}";
      var wpSiteName = "{{ site_name }}";
      var gaIdLabel = "{{ gtag_id }}/{{ gtag_label }}";
    </script>

    <script src="{{ site.theme.link }}/assets/js/script.js?v={{
        asset_version
      }}"></script>

    <!-- Start of HubSpot Embed Code -->
    <script
      type="text/javascript"
      id="hs-script-loader"
      async
      defer
      src="//js.hs-scripts.com/9300456.js"
    ></script>
    <!-- End of HubSpot Embed Code -->

    <!-- Google Tag Manager -->
    <script>
      (function (w, d, s, l, i) {
        w[l] = w[l] || [];
        w[l].push({
          "gtm.start": new Date().getTime(),
          event: "gtm.js",
        });
        var f = d.getElementsByTagName(s)[0],
          j = d.createElement(s),
          dl = l != "dataLayer" ? "&l=" + l : "";
        j.async = true;
        j.src = "https://www.googletagmanager.com/gtm.js?id=" + i + dl;
        f.parentNode.insertBefore(j, f);
      })(window, document, "script", "dataLayer", "GTM-W2TC9NP");
    </script>
    <!-- End Google Tag Manager -->

    <!-- Facebook Pixel Code -->
    <script>
      !(function (f, b, e, v, n, t, s) {
        if (f.fbq) return;
        n = f.fbq = function () {
          n.callMethod
            ? n.callMethod.apply(n, arguments)
            : n.queue.push(arguments);
        };
        if (!f._fbq) f._fbq = n;
        n.push = n;
        n.loaded = !0;
        n.version = "2.0";
        n.queue = [];
        t = b.createElement(e);
        t.async = !0;
        t.src = v;
        s = b.getElementsByTagName(e)[0];
        s.parentNode.insertBefore(t, s);
      })(
        window,
        document,
        "script",
        "https://connect.facebook.net/en_US/fbevents.js"
      );
      fbq("init", "209704970774476");
      fbq("track", "PageView");
    </script>
    <noscript
      ><img
        height="1"
        width="1"
        style="display: none"
        src="https://www.facebook.com/tr?id=209704970774476&ev=PageView&noscript=1"
    /></noscript>
    <!-- End Facebook Pixel Code -->

    {{
      function("wp_head")
    }}

    {% if sumome %}
    <script async>
      (function (s, u, m, o, j, v) {
        j = u.createElement(m);
        v = u.getElementsByTagName(m)[0];
        j.async = 1;
        j.src = o;
        j.dataset.sumoSiteId =
          "3488204638f26afdd50d256c18ca2903e9e9fea6a4af904c45581ea75ca5cb89";
        v.parentNode.insertBefore(j, v);
      })(window, document, "script", "//load.sumo.com/");
    </script>
    {% endif %}

    <style>
      .footer-heading-container {
        display: flex;
        align-items: center;
        justify-content: center;
      }
      .footer-heading {
        font-weight: bold;
        text-align: center;
        font-size: 40px;
        line-height: 40.2px;
        margin: 0px;
        padding-bottom: 50px;
      }

      @media (min-width: 768px) {
        .footer-heading {
          font-size: 40px;
        }
      }
      @media (min-width: 992px) {
        .fixed-top-margin {
          margin-top: 40px;
        }
      }
      @media (min-width: 1440px) {
        .fixed-top-margin {
          margin-top: 80px;
        }
      }
    </style>
  </head>
  {% endblock %}

  <body class="{{ body_class }}">
    <!-- Google Tag Manager (noscript) -->
    <noscript
      ><iframe
        src="https://www.googletagmanager.com/ns.html?id=GTM-W2TC9NP"
        height="0"
        width="0"
        style="display: none; visibility: hidden"
      ></iframe
    ></noscript>
    <!-- End Google Tag Manager (noscript) -->

    {% block header %}
    <!-- Header -->

    <nav
      class="navbar fixed-top fixed-top-no-nav navbar-expand-lg navbar-header fixed-top-margin"
    >
      <div class="container">
        <!-- Navigation Toggler -->
        {# <button
          class="navbar-toggler d-none"
          type="button"
          data-toggle="collapse"
          data-target="#navbarSupportedContent"
          aria-controls="navbarSupportedContent"
          aria-expanded="false"
          aria-label="Toggle navigation"
        >
          <i class="fa fa-bars" aria-hidden="true"></i>
        </button> #}

        <!-- Logo -->
        <a class="navbar-brand" href="{{ home_url }}">
          <img
            src="{{ site.theme.link }}/assets/img/fp-logo.png"
            srcset="{{ site.theme.link }}/assets/img/fp-logo@2x.png 2x"
            alt="First Page"
          />
        </a>

        <!-- Navigation -->
        <div class="collapse navbar-collapse" id="navbarSupportedContent">
          <!-- Contact -->
          <div class="additional-menu">
            <button
              class="navbar-toggler navbar-toggler-close collapsed"
              type="button"
              data-toggle="collapse"
              data-target="#navbarSupportedContent"
              aria-controls="navbarSupportedContent"
              aria-expanded="false"
              aria-label="Toggle navigation"
            >
              <i class="fa fa-times" aria-hidden="true"></i>
            </button>
          </div>

          <!-- Menu -->
          <div class="menu mr-auto">
            <ul class="navbar-nav mr-auto">
              <!-- <li class="nav-item nav-item-main d-none d-lg-block"> -->
              <li class="nav-item nav-item-main d-none d-lg-block">
                <a class="scroll-nav-logo" href="{{ home_url }}">
                  <img
                    id="scroll-nav-logo"
                    src="{{ site.theme.link }}/assets/img/fp-logo.png"
                    srcset="{{ site.theme.link }}/assets/img/fp-logo@2x.png 2x"
                    alt="First Page"
                /></a>
              </li>
              
            </ul>
          </div>
        </div>
      </div>
    </nav>
    {% endblock %} {% block content %} {% endblock %} {% block footer %}
    <!-- Footer -->
    {# <footer>
      <div class="container footer-heading-container">
        <p class="footer-heading">We have the answers.</p>
      </div>

    </footer> #}

    <section class="copyright">
      <div class="container">
        <div class="row align-items-center no-gutters">
          <div class="text col-12 col-xl-3">
            {% block copyright %} Copyright &copy; {{ "now"|date("Y") }} First
            Page. {% endblock %}<br />
            <p >Terms of Business</p>
          </div>
          <div class="logos col-12 col-xl-9">
            <img
              src="{{ site.theme.link }}/assets/img/img_new_footer_sm.png"
              srcset="{{ site.theme.link }}/assets/img/img_new_footer_sm.png, {{ site.theme.link }}/assets/img/img_new_footer_sm@2x.png 2x"
              class="img-fluid d-md-none"
              alt="We work with great brands of all sizes"
            />

            <img
              src="{{ site.theme.link }}/assets/img/img_new_footer_md.png"
              srcset="{{ site.theme.link }}/assets/img/img_new_footer_md.png, {{ site.theme.link }}/assets/img/img_new_footer_md@2x.png 2x"
              class="img-fluid d-none d-md-inline-block d-lg-none"
              alt="We work with great brands of all sizes"
            />

            <img
              src="{{ site.theme.link }}/assets/img/img_new_footer_lg.png"
              srcset="{{ site.theme.link }}/assets/img/img_new_footer_lg.png, {{ site.theme.link }}/assets/img/img_new_footer_lg@2x.png 2x"
              class="img-fluid d-none d-lg-inline-block"
              alt="We work with great brands of all sizes"
            />
          </div>
        </div>
      </div>
    </section>

    <div
      id="get-proposal-popup"
      class="get-proposal-popup white-popup mfp-hide"
    >
      <div class="quote-form quote-form-big">
        <!-- Floating Form -->
        <form class="fp-form" method="POST">
          <div class="form-step-1 form-step">
            <h3>Get Started</h3>
            <div class="row form-row">
              <div class="col-12">
                <input
                  type="text"
                  class="form-control"
                  name="lead_website"
                  placeholder="Enter Your Website*"
                  required=""
                />
              </div>
              <div class="col-12">
                <button
                  type="button"
                  class="btn fp-btn fp-btn-orange fp-btn-shadow form-continue"
                >
                  Continue
                </button>
              </div>
            </div>
          </div>
          <div class="form-step-2 form-step">
            <h4>Enter your details</h4>
            <div class="row form-row">
              <div class="col-12">
                <input
                  type="text"
                  class="form-control"
                  name="lead_name"
                  placeholder="First Name*"
                  required=""
                />
              </div>
              <div class="col-12">
                <input
                  type="email"
                  class="form-control"
                  name="lead_email"
                  placeholder="Email*"
                  data-parsley-error-message="Please enter a valid email address"
                  data-parsley-trigger="change"
                  required=""
                />
              </div>
              <div class="col-12">
                <input
                  type="tel"
                  class="form-control"
                  name="lead_phone"
                  placeholder="Phone*"
                  required=""
                />
              </div>
              <div class="col-12">
                <input
                  type="hidden"
                  name="lead_formname"
                  value="Get Proposal Form"
                />
                <input type="hidden" name="lead_language" value="en" />
                <input
                  type="hidden"
                  name="lead_formtype"
                  value="hs_quote_form"
                />
                <button
                  type="submit"
                  class="form-control btn fp-btn fp-btn-orange fp-btn-shadow"
                  onclick="ga('send','event', {'eventCategory': 'Form','eventAction': 'Click', 'eventLabel': 'Get Proposal Form'});"
                >
                  Get A Free Proposal
                </button>
              </div>
            </div>
          </div>
          <div class="form-thankyou">
            <h3>Thank You!</h3>
            <p>
              Your Digital Marketing Specialist will be in touch within 24
              hours. Alternatively, for an instant chat, please call
              <a href="tel:{{ site_phone|replace({' ':''}) }}"
                ><b>{{ site_phone }}</b></a
              >
            </p>
          </div>
        </form>
        <!-- End of Form -->
      </div>
    </div>

    {{ function("wp_footer") }}
    {% endblock %}
  </body>
</html>
