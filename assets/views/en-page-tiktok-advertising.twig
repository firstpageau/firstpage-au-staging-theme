{% block content %}

<style>
    @media (max-width: 767px) {
        .page-tiktok-advertising .hero-mobile {
            bottom: 29px;
            right: -15px;
        }

        .page-tiktok-advertising section.banner .banner-description p {
            max-width: 195px;
        }
    }
    
    @media(min-width: 768px){
        section.banner .banner-hero .hero-img {
            right: -54px;
        }
    }
    @media(min-width: 992px){
        section.banner .banner-hero .hero-img {
            right: -199px;
        }
    }
    @media(min-width: 1024px){
        section.banner .banner-hero .hero-img {
            right: -235px;
        }
